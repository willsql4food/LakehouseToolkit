{
	"name": "OnpremSqlDb",
	"properties": {
		"parameters": {
			"serverFqdn": {
				"type": "string",
				"defaultValue": "localhost\\ACB"
			},
			"dbName": {
				"type": "string",
				"defaultValue": "master"
			},
			"userName": {
				"type": "string",
				"defaultValue": "ab092898@flooranddecor.com"
			},
			"akvBaseUrl": {
				"type": "string",
				"defaultValue": "https://akv-ab092898.vault.azure.net/"
			},
			"PwdSecret": {
				"type": "string",
				"defaultValue": "WinAuthPwd"
			}
		},
		"annotations": [],
		"type": "SqlServer",
		"typeProperties": {
			"connectionString": "Integrated Security=True;Data Source=@{linkedService().serverFqdn};Initial Catalog=@{linkedService().dbName}",
			"userName": "@{linkedService().userName}",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzKeyVault",
					"type": "LinkedServiceReference",
					"parameters": {
						"baseUrl": {
							"value": "@linkedService().akvBaseUrl",
							"type": "Expression"
						}
					}
				},
				"secretName": {
					"value": "@linkedService().PwdSecret",
					"type": "Expression"
				}
			}
		},
		"connectVia": {
			"referenceName": "shirt-ACB",
			"type": "IntegrationRuntimeReference"
		}
	},
	"type": "Microsoft.DataFactory/factories/linkedservices"
}