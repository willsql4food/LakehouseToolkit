							"schemaName": "dbo",
							"tableName": "Case",
							"sqlFqdn": "localhost\\ACB",
							"dbName": "SevenLetterTees",
							"userName": "AB092898@flooranddecor.com",
							"pwdSecretName": "WinAuthPwd",
							"akvBaseUrl": "https://akv-ab092898.vault.azure.net/"
						}
					}
				]
			}
		],
		"annotations": []
	}
